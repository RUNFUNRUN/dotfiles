syntax on
set number
set clipboard+=unnamed
set tabstop=2
set shiftwidth=2
set smartindent
set expandtab
set incsearch
set hlsearch
set autoindent
set encoding=utf-8
set autoread
set nobackup
set nowritebackup
set noswapfile
set cursorline
set laststatus=2
set mouse=a
set nowrap
inoremap jj <Esc>
nnoremap <Esc><Esc> :noh<CR>
highlight Normal ctermfg=white ctermbg=16
highlight Comment ctermfg=lightblue ctermbg=16
highlight Constant ctermfg=lightyellow ctermbg=16
highlight Identifier ctermfg=lightcyan ctermbg=16
highlight Statement ctermfg=lightmagenta ctermbg=16
highlight PreProc ctermfg=lightgreen ctermbg=16
highlight Type ctermfg=lightred ctermbg=16
highlight Special ctermfg=lightmagenta ctermbg=16
highlight Underlined ctermfg=white ctermbg=16
highlight CursorLine cterm=NONE ctermbg=234
highlight LineNr ctermfg=240
highlight CursorLineNr ctermfg=250
